{
  "name": "eTracker chrome extension",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["event.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "webNavigation",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "storage",
    "identity"  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
